    <style>
      /* Modern SEO Analyzer Styles - State of the Art */
      
      /* Animated gradient background */
      body {
        position: relative;
        overflow-x: hidden;
      }
      
      body::before {
        content: '';
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: 
          radial-gradient(circle at 20% 50%, rgba(0, 255, 65, 0.03) 0%, transparent 50%),
          radial-gradient(circle at 80% 80%, rgba(0, 204, 255, 0.03) 0%, transparent 50%),
          radial-gradient(circle at 40% 80%, rgba(138, 43, 226, 0.02) 0%, transparent 50%);
        animation: gradientShift 15s ease infinite;
        pointer-events: none;
        z-index: 0;
      }
      
      @keyframes gradientShift {
        0%, 100% { opacity: 1; transform: scale(1) rotate(0deg); }
        50% { opacity: 0.8; transform: scale(1.1) rotate(5deg); }
      }
      
      .container {
        position: relative;
        z-index: 1;
      }
      
      /* Hero section with glassmorphism */
      .hero-section {
        background: linear-gradient(135deg, rgba(0, 255, 65, 0.05) 0%, rgba(0, 204, 51, 0.05) 100%);
        backdrop-filter: blur(20px);
        border: 1px solid rgba(0, 255, 65, 0.2);
        border-radius: 20px;
        padding: 3rem 2rem;
        margin: 2rem 0;
        position: relative;
        overflow: hidden;
        box-shadow: 
          0 10px 40px rgba(0, 255, 65, 0.1),
          inset 0 1px 0 rgba(255, 255, 255, 0.1);
      }
      
      .hero-section::before {
        content: '';
        position: absolute;
        top: -50%;
        left: -50%;
        width: 200%;
        height: 200%;
        background: linear-gradient(45deg, transparent, rgba(0, 255, 65, 0.03), transparent);
        animation: shimmer 3s infinite;
      }
      
      @keyframes shimmer {
        0% { transform: translateX(-100%) translateY(-100%) rotate(45deg); }
        100% { transform: translateX(100%) translateY(100%) rotate(45deg); }
      }
      
      .hero-content {
        position: relative;
        z-index: 1;
      }
      
      .hero-title {
        font-size: 2.5rem;
        font-weight: 700;
        text-align: center;
        background: linear-gradient(135deg, #00ff41 0%, #00cc33 50%, #00ff41 100%);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        margin-bottom: 1rem;
        animation: titleGlow 3s ease-in-out infinite;
      }
      
      @keyframes titleGlow {
        0%, 100% { filter: drop-shadow(0 0 10px rgba(0, 255, 65, 0.5)); }
        50% { filter: drop-shadow(0 0 20px rgba(0, 255, 65, 0.8)); }
      }
      
      .hero-subtitle {
        font-size: 1.1rem;
        text-align: center;
        color: #00ff41;
        margin-bottom: 2rem;
        text-shadow: 0 0 10px rgba(0, 255, 65, 0.5);
      }
      
      /* Feature list with modern cards */
      .feature-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
        gap: 1.5rem;
        margin: 2rem 0;
      }
      
      .feature-card {
        background: rgba(255, 255, 255, 0.02);
        backdrop-filter: blur(10px);
        border: 1px solid rgba(0, 255, 65, 0.2);
        border-radius: 12px;
        padding: 1.5rem;
        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
        position: relative;
        overflow: hidden;
        cursor: pointer;
      }
      
      .feature-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(135deg, rgba(0, 255, 65, 0.1) 0%, transparent 100%);
        opacity: 0;
        transition: opacity 0.4s ease;
      }
      
      .feature-card:hover {
        transform: translateY(-5px) scale(1.02);
        border-color: rgba(0, 255, 65, 0.5);
        box-shadow: 
          0 20px 40px rgba(0, 255, 65, 0.2),
          inset 0 1px 0 rgba(255, 255, 255, 0.1);
      }
      
      .feature-card:hover::before {
        opacity: 1;
      }
      
      .feature-icon {
        font-size: 2rem;
        margin-bottom: 0.5rem;
        display: inline-block;
        animation: float 3s ease-in-out infinite;
      }
      
      @keyframes float {
        0%, 100% { transform: translateY(0); }
        50% { transform: translateY(-10px); }
      }
      
      .feature-title {
        font-size: 1rem;
        font-weight: 600;
        color: #00ff41;
        margin-bottom: 0.5rem;
      }
      
      .feature-description {
        font-size: 0.9rem;
        color: #b0b0b0;
        line-height: 1.5;
      }
      
      /* SEO Score with 3D effect */
      .seo-score-container {
        perspective: 1000px;
        margin: 2rem 0;
      }
      
      .seo-score {
        font-size: 4rem;
        font-weight: 700;
        text-align: center;
        margin: 2rem 0;
        background: linear-gradient(135deg, #00ff41 0%, #00cc33 100%);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        filter: drop-shadow(0 0 30px rgba(0, 255, 65, 0.6));
        animation: scoreAppear 1s ease-out, scorePulse 2s ease-in-out infinite 1s;
        transform-style: preserve-3d;
      }
      
      @keyframes scoreAppear {
        0% { opacity: 0; transform: scale(0.5) rotateX(-90deg); }
        100% { opacity: 1; transform: scale(1) rotateX(0deg); }
      }
      
      @keyframes scorePulse {
        0%, 100% { transform: scale(1); }
        50% { transform: scale(1.05); }
      }
      
      .seo-score.excellent { 
        background: linear-gradient(135deg, #00ff41 0%, #00cc33 100%);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
      }
      
      /* Modern category cards */
      .seo-category {
        background: rgba(255, 255, 255, 0.02);
        backdrop-filter: blur(15px);
        border: 1px solid rgba(255, 255, 255, 0.1);
        border-radius: 16px;
        padding: 2rem;
        margin-bottom: 2rem;
        position: relative;
        overflow: hidden;
        transition: all 0.4s ease;
      }
      
      .seo-category::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        height: 3px;
        background: linear-gradient(90deg, #00ff41, #00cc33, #00ff41);
        background-size: 200% 100%;
        animation: gradientMove 3s linear infinite;
      }
      
      @keyframes gradientMove {
        0% { background-position: 0% 50%; }
        100% { background-position: 200% 50%; }
      }
      
      .seo-category:hover {
        transform: translateY(-2px);
        border-color: rgba(0, 255, 65, 0.3);
        box-shadow: 0 15px 35px rgba(0, 255, 65, 0.15);
      }
      
      .seo-category h3 {
        display: flex;
        align-items: center;
        gap: 0.5rem;
        font-size: 1.3rem;
        margin-bottom: 1.5rem;
        color: #00ff41;
      }
      
      /* Modern SEO items */
      .seo-item {
        display: flex;
        align-items: flex-start;
        gap: 1rem;
        padding: 1rem;
        margin: 0.75rem 0;
        background: rgba(0, 0, 0, 0.3);
        border-radius: 8px;
        border-left: 3px solid transparent;
        transition: all 0.3s ease;
        position: relative;
        overflow: hidden;
      }
      
      .seo-item::before {
        content: '';
        position: absolute;
        left: 0;
        top: 0;
        bottom: 0;
        width: 3px;
        transition: width 0.3s ease;
      }
      
      .seo-item:hover {
        background: rgba(0, 0, 0, 0.4);
        transform: translateX(5px);
        box-shadow: -5px 0 15px rgba(0, 255, 65, 0.1);
      }
      
      .seo-item.pass { border-left-color: #4caf50; }
      .seo-item.pass::before { background: #4caf50; }
      .seo-item.pass:hover::before { width: 100%; opacity: 0.05; }
      
      .seo-item.warning { border-left-color: #ff9800; }
      .seo-item.warning::before { background: #ff9800; }
      .seo-item.warning:hover::before { width: 100%; opacity: 0.05; }
      
      .seo-item.fail { border-left-color: #f44336; }
      .seo-item.fail::before { background: #f44336; }
      .seo-item.fail:hover::before { width: 100%; opacity: 0.05; }
      
      .seo-icon {
        font-size: 1.5rem;
        min-width: 30px;
        animation: iconBounce 2s ease-in-out infinite;
      }
      
      @keyframes iconBounce {
        0%, 100% { transform: scale(1); }
        50% { transform: scale(1.1); }
      }
      
      /* Stats bar */
      .stats-bar {
        display: flex;
        justify-content: space-around;
        gap: 1rem;
        margin: 2rem 0;
        flex-wrap: wrap;
      }
      
      .stat-card {
        flex: 1;
        min-width: 200px;
        background: rgba(0, 255, 65, 0.05);
        border: 1px solid rgba(0, 255, 65, 0.2);
        border-radius: 12px;
        padding: 1.5rem;
        text-align: center;
        transition: all 0.3s ease;
      }
      
      .stat-card:hover {
        transform: translateY(-5px);
        background: rgba(0, 255, 65, 0.08);
        box-shadow: 0 10px 30px rgba(0, 255, 65, 0.2);
      }
      
      .stat-value {
        font-size: 2rem;
        font-weight: 700;
        color: #00ff41;
        margin-bottom: 0.5rem;
      }
      
      .stat-label {
        font-size: 0.9rem;
        color: #808080;
        text-transform: uppercase;
        letter-spacing: 1px;
      }
      
      /* Coming soon badge */
      .coming-soon-badge {
        display: inline-block;
        background: linear-gradient(135deg, #ff0080, #ff8c00);
        color: white;
        font-size: 0.8rem;
        font-weight: 700;
        padding: 0.5rem 1rem;
        border-radius: 20px;
        text-transform: uppercase;
        letter-spacing: 1px;
        animation: badgePulse 2s ease-in-out infinite;
        box-shadow: 0 5px 15px rgba(255, 0, 128, 0.4);
      }
      
      @keyframes badgePulse {
        0%, 100% { transform: scale(1); box-shadow: 0 5px 15px rgba(255, 0, 128, 0.4); }
        50% { transform: scale(1.05); box-shadow: 0 7px 20px rgba(255, 0, 128, 0.6); }
      }
      
      /* Responsive design */
      @media (max-width: 768px) {
        .hero-title { font-size: 2rem; }
        .seo-score { font-size: 3rem; }
        .feature-grid { grid-template-columns: 1fr; }
        .stats-bar { flex-direction: column; }
      }

      /* SEO Results Styles */
      .seo-results-wrapper { margin-top: 3rem; animation: fadeIn 0.5s ease-in; }
      .seo-score-section { text-align: center; padding: 3rem 2rem; background: rgba(0, 255, 65, 0.03); border: 1px solid rgba(0, 255, 65, 0.2); border-radius: 12px; margin-bottom: 3rem; }
      .seo-score-circle { width: 200px; height: 200px; margin: 2rem auto; border-radius: 50%; display: flex; flex-direction: column; align-items: center; justify-content: center; background: rgba(0, 0, 0, 0.5); border: 4px solid; box-shadow: 0 0 30px rgba(0, 255, 65, 0.3); }
      .seo-score-circle.score-a { border-color: #00ff41; box-shadow: 0 0 30px rgba(0, 255, 65, 0.5); }
      .seo-score-circle.score-b { border-color: #00cc33; }
      .seo-score-circle.score-c { border-color: #ff8c00; }
      .seo-score-circle.score-d { border-color: #ff6600; }
      .seo-score-circle.score-f { border-color: #ff0000; }
      .score-number { font-size: 4rem; font-weight: 700; background: linear-gradient(135deg, #00ff41 0%, #00cc33 100%); -webkit-background-clip: text; -webkit-text-fill-color: transparent; }
      .score-grade { font-size: 1.5rem; color: #00ff41; font-weight: 600; }
      .score-breakdown { display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 1rem; margin-top: 2rem; }
      .score-component { padding: 0.75rem 1rem; background: rgba(255, 255, 255, 0.02); border: 1px solid rgba(255, 255, 255, 0.1); border-radius: 6px; display: flex; justify-content: space-between; }
      .component-name { color: #b0b0b0; font-size: 0.9rem; }
      .component-score { color: #00ff41; font-weight: 600; }
      .seo-section { margin-bottom: 2rem; padding: 2rem; background: rgba(255, 255, 255, 0.02); border: 1px solid; border-radius: 12px; transition: all 0.3s ease; }
      .seo-section.passed { border-color: rgba(0, 255, 65, 0.3); }
      .seo-section.issues { border-color: rgba(255, 140, 0, 0.3); }
      .seo-section:hover { transform: translateY(-2px); box-shadow: 0 5px 20px rgba(0, 255, 65, 0.1); }
      .section-header { display: flex; justify-content: space-between; align-items: center; margin-bottom: 1.5rem; padding-bottom: 1rem; border-bottom: 1px solid rgba(255, 255, 255, 0.1); }
      .section-header h3 { margin: 0; color: #00ff41; font-size: 1.5rem; }
      .section-score { font-size: 1.5rem; font-weight: 700; color: #00ff41; padding: 0.25rem 1rem; background: rgba(0, 255, 65, 0.1); border-radius: 20px; }
      .section-content { color: #d0d0d0; }
      .meta-item, .meta-group { margin-bottom: 1rem; padding: 0.75rem; background: rgba(0, 0, 0, 0.3); border-radius: 6px; }
      .meta-item strong, .meta-group strong { color: #00ff41; display: block; margin-bottom: 0.5rem; }
      .meta-length { display: inline-block; padding: 0.2rem 0.5rem; border-radius: 4px; font-size: 0.85rem; margin-left: 0.5rem; }
      .meta-length.good { background: rgba(0, 255, 65, 0.2); color: #00ff41; }
      .meta-length.warning { background: rgba(255, 140, 0, 0.2); color: #ff8c00; }
      .meta-length.poor { background: rgba(255, 0, 0, 0.2); color: #ff0000; }
      .missing-value { color: #ff6600; font-style: italic; }
      .heading-stats { display: grid; grid-template-columns: repeat(auto-fit, minmax(120px, 1fr)); gap: 1rem; margin-bottom: 1.5rem; }
      .heading-stat { padding: 1rem; background: rgba(0, 0, 0, 0.3); border-radius: 6px; text-align: center; }
      .heading-stat strong { display: block; color: #00ff41; margin-bottom: 0.5rem; }
      .content-stats-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(150px, 1fr)); gap: 1.5rem; margin-bottom: 1.5rem; }
      .content-stat { text-align: center; padding: 1.5rem 1rem; background: rgba(0, 0, 0, 0.3); border-radius: 8px; }
      .stat-label { display: block; color: #808080; font-size: 0.85rem; margin-bottom: 0.5rem; }
      .stat-value { display: block; font-size: 2rem; font-weight: 700; }
      .stat-value.good { color: #00ff41; }
      .stat-value.warning { color: #ff8c00; }
      .stat-value.poor { color: #ff6600; }
      .image-item { padding: 0.75rem; background: rgba(0, 0, 0, 0.3); border-radius: 6px; margin-bottom: 0.75rem; }
      .image-item.has-alt { border-left: 3px solid #00ff41; }
      .image-item.no-alt { border-left: 3px solid #ff6600; }
      .issues-list { margin-top: 1.5rem; padding: 1rem; background: rgba(255, 102, 0, 0.1); border-left: 4px solid #ff6600; border-radius: 8px; }
      .recommendations-list { margin-top: 1.5rem; padding: 1rem; background: rgba(0, 204, 255, 0.1); border-left: 4px solid #00ccff; border-radius: 8px; }
      .issues-list ul, .recommendations-list ul { margin: 0.5rem 0 0 1rem; }
      .seo-loading { text-align: center; padding: 4rem 2rem; }
      .loading-spinner { width: 60px; height: 60px; border: 4px solid rgba(0, 255, 65, 0.2); border-top-color: #00ff41; border-radius: 50%; margin: 0 auto 2rem; animation: spin 1s linear infinite; }
      @keyframes spin { to { transform: rotate(360deg); } }
      .seo-error { text-align: center; padding: 4rem 2rem; background: rgba(255, 0, 0, 0.1); border: 1px solid rgba(255, 0, 0, 0.3); border-radius: 12px; }
      .error-icon { font-size: 4rem; margin-bottom: 1rem; }
      .seo-error button { margin-top: 2rem; padding: 0.75rem 2rem; background: #00ff41; color: #000; border: none; border-radius: 6px; font-weight: 600; cursor: pointer; }
      .seo-summary { margin-top: 3rem; padding: 2rem; background: rgba(0, 255, 65, 0.05); border: 1px solid rgba(0, 255, 65, 0.2); border-radius: 12px; }
      @keyframes fadeIn { from { opacity: 0; transform: translateY(20px); } to { opacity: 1; transform: translateY(0); } }
    </style>
  </head>
